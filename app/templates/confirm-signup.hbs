{{model}}
<Form @model={{this}}
  @classNames="md:w-1/2 lg:w-1/3 mx-auto"
  @onSubmit={{action "confirm"}} as |f|>
  <f.legend>Confirm Signup</f.legend>
  <f.errors @errors={{this.errors}} />
  <f.input @field="username" />
  <f.input @field="code"  />
  <div class="w-full mb-6 px-3">
    <f.button @type="submit">Submit</f.button>
  </div>
</Form>
